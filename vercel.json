{
    "builds": [
        {
            "src":"app.py",
            "use":"@vercel/python",
            "config": {
                "runtime": "python3.9",
                "requirementsPath": "requirements-vercel.txt",
                "maxLambdaSize": "50mb"
            }
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src":"/(.*)", 
            "dest":"app.py"
        }
    ]
}